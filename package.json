{
  "name": "ademag",
  "version": "1.0.0",
  "description": "Back end du site ademag",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production --progress",
    "start": "npm run build && node src/server/bin/www",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/bin/www",
    "dev": "concurrently  \"npm run client\" \"npm run server\"",
    "test": "nodemon --exec \"mocha -R min\"",
    "dt": "concurrently \"npm run client\" \"npm run test\" \"npm run server\"",
    "clean": "rd /S /Q node_modules && del /F package-lock.json && npm install",
    "reset": "rm -rf node_modules/ && rm -rf package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andresvcc/ademag.git"
  },
  "author": "andres caballero",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andresvcc/ademag/issues"
  },
  "homepage": "https://github.com/andresvcc/ademag#readme",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.16.2",
    "helmet": "^3.20.0",
    "mongoose": "^5.6.10",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "sockjs": "^0.3.19",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.12.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.17.3",
    "react-scripts": "latest",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.8.0"
